    "# Performs the alignment of the different observations images of the same detector\n",
    "def astro_align(path, savepath):\n",
    "    \n",
    "    files = [f for f in os.listdir(path)]\n",
    "    files = np.sort(files)\n",
    "    \n",
    "    #read the first image\n",
    "    hdu = fits.open(path + files[0])\n",
    "    header1 = hdu[0].header #reads the header\n",
    "    im1 = hdu[0].data #read the image\n",
    "    j = 0\n",
    "    for j in range(3):\n",
    "        im1[j,:] = np.nan\n",
    "        im1[:,j] = np.nan\n",
    "        k = 4175 - j #Index ends at 4175, len is 4176\n",
    "        l = 2043 - j #Index ends at 2043, len is 2045\n",
    "        im1[k,:] = np.nan\n",
    "        im1[:,l] = np.nan\n",
    "    \n",
    "    #To account for the shifts of the images\n",
    "    pad = 400 #this many pixels from each side\n",
    "    #initiate the cube with nans\n",
    "    cube = np.zeros(shape = (len(files), int(header1['NAXIS2']) + 2 * pad, int(header1['NAXIS1']) + 2 * pad), dtype = float)\n",
    "    cube[:] = np.nan\n",
    "    #The first slice of the cube is the first image, and this will be a reference for shifting the rest\n",
    "    cube[0, pad:pad+header1['NAXIS2'], pad:pad+header1['NAXIS1']] = im1\n",
    "    header1['CRPIX1'] = header1['CRPIX1'] + pad\n",
    "    header1['CRPIX2'] = header1['CRPIX2'] + pad\n",
    "    \n",
    "    #loop over the remaining 4 images\n",
    "    for i in range(1,len(files)):\n",
    "        hdu = fits.open(path + files[i])\n",
    "        header2 = hdu[0].header #reads the header\n",
    "        im2 = hdu[0].data #read the image\n",
    "        m = 0\n",
    "        for m in range(3):\n",
    "            im2[m,:] = np.nan\n",
    "            im2[:,m] = np.nan\n",
    "            n = 4175 - m\n",
    "            o = 2043 - m\n",
    "            im2[n,:] = np.nan\n",
    "            im2[:,o] = np.nan\n",
    "        cube[i, pad:pad+header1['NAXIS2'], pad:pad+header1['NAXIS1']] = im2\n",
    "        #Use astroalign for a first order transformation (not perefct, but good enough to match individual stars)\n",
    "        #number 2 transformed to 1 coords\n",
    "        p, (pos_img, pos_img_rot) = aa.find_transform(im2, im1)\n",
    "        img_aligned, footprint = aa.apply_transform(p, cube[i,:,:], cube[0,:,:], fill_value = np.nan)\n",
    "        \n",
    "        #Here I'm writing the first image as tmp1.fits, and the second one which \n",
    "        #is now roughy aligned with it as tmp2.fits\n",
    "        fits.writeto('tmp1.fits', cube[0,:,:], header = header1, overwrite = True)\n",
    "        fits.writeto('tmp2.fits', img_aligned, header = header1, overwrite = True)\n",
    "        \n",
    "        #Call sextractor ionthe two images. The catalog is saved as tmp.cat, and the background is saved as back.fits\n",
    "        #in your sextractor conf. file (cra.sex here), you should have:\n",
    "        #CHECKIMAGE_TYPE BACKGROUND       \n",
    "        #CHECKIMAGE_NAME  back.fits  \n",
    "        #We will use this estimate of the background to scale the images before combining them with a median\n",
    "        #Also, I set CATALOG_TYPE ASCII_HEAD which produces an ascii catalog with a header (names of the columns) \n",
    "        os.system('sextractor tmp1.fits -c cra.sex')\n",
    "        os.system('mv tmp.cat tmp1.cat')\n",
    "        bkg = fits.getdata('back.fits')\n",
    "        bkg1 = np.nanmedian(bkg)\n",
    "        os.system('sextractor tmp2.fits -c cra.sex')\n",
    "        os.system('mv tmp.cat tmp2.cat')\n",
    "        bkg = fits.getdata('back.fits')\n",
    "        bkg2 = np.nanmedian(bkg) \n",
    "        \n",
    "        cat1 = np.loadtxt('tmp1.cat', dtype = object) \n",
    "        cat2 = np.loadtxt('tmp2.cat', dtype = object) \n",
    "        \n",
    "        #Convert ra and dec for the two catalogs into format good for matching\n",
    "        c1 = SkyCoord(ra = cat1[:,3].astype(float) * u.degree, dec = cat1[:,4].astype(float) * u.degree)\n",
    "        c2 = SkyCoord(ra = cat2[:,3].astype(float) * u.degree, dec = cat2[:,4].astype(float) * u.degree)\n",
    "        \n",
    "        #match the catalog1 (c1) to catalog2 (c2)\n",
    "        idx, d2d, d3d = c1.match_to_catalog_sky(c2)\n",
    "        max_sep = 2.0 * u.arcsec #you can adjust this if necessary\n",
    "        idx_sep = d2d < max_sep #all sources that have separation below max_sep\n",
    "        cat1_m = cat1[idx_sep] #matched from cat1\n",
    "        cat2_m = cat2[idx[idx_sep]] #matched from cat2\n",
    "        \n",
    "        m1 = cat1_m[:,1:3].astype(float)\n",
    "        m2 = cat2_m[:,1:3].astype(float)\n",
    "        #This will now calculate a transformation using the pixel coordinates of the matched stars\n",
    "        #This is much more precise than  the previous alignment\n",
    "        tform = aa.estimate_transform('affine', m2, m1)\n",
    "        img_aligned2, footprint = aa.apply_transform(tform, img_aligned, cube[0,:,:], fill_value = np.nan)\n",
    "        cube[i,:,:] = img_aligned2 * bkg1 / bkg2 #save as a cube slice, with scaled background\n",
    "    \n",
    "    print('Cube dimensions:', cube.shape)\n",
    "    #save the cube if you wish (good for checking that all is fine)\n",
    "    cubepath = savepath + 'Cube_Images/'\n",
    "    if not os.path.exists(cubepath):\n",
    "        os.makedirs(cubepath)\n",
    "    fits.writeto(cubepath + header1['FILTER01'] + \"_\" + header1['DETECTOR'] + '_cube.fits', cube, overwrite = True)\n",
    "    im_final = np.nanmedian(cube, 0)   \n",
    "    #Write the final image in a temporary fits. Note that the header here is off, \n",
    "    #but I save it so that astrometry.net has an initial point to look for the solution\n",
    "    tmppath = savepath + 'Stacked_Temp_Images/'\n",
    "    if not os.path.exists(tmppath):\n",
    "        os.makedirs(tmppath)\n",
    "    fits.writeto(tmppath + header1['FILTER01'] + \"_\" + header1['DETECTOR'] + '_stacked_tmp.fits', im_final, header = header1, overwrite = True)\n",
   ]
  }
